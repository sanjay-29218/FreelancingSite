
	
	

<div class="container section">
	<h1>
		MY Vacancies
	</h1>
		
		{{#each vacancy}}
		<div class="col-md-3">
			<div class="gig-card">
				<a href="/vacancy_detail_/{{ _id }}">
					<img src="/img/copywrite.jpg" />
					<h4>{{title}}</h4>
					<h3>${{price}}</h3>
				</a>
				<div class="editbtn" >
				<a href = "/edit_vacancy/{{_id}}"><i  class = "fa fa-pencil fa-lg " aria-hidden = "true" ></i> Edit </a>
                </div>
                <div class="delbtn"
				<a href="/del_vacancy/{{_id}}"> <i  class = " fa fa-trash fa-lg"  aria-hidden= "true"></i> Delete </a>
                </div>
				
			</div>
		</div>
		{{/each}}
		
	</div>
  
</div>