
	
	

<div class="container section">
	<h1>
		MY Jobs
	</h1>
		
		{{#each gigs}}
		<div class="col-md-3">
			<div class="gig-card">
				<a href="/service_detail_/{{ _id }}">
					<img src="/img/copywrite.jpg" />
					<h4>{{title}}</h4>
					<h3>${{price}}</h3>
				<div >
					<div class="editbtn" >
				<a href = "/edit_gigs/{{_id}}"><i  class = "fa fa-pencil fa-lg " aria-hidden = "true" ></i> Edit </a></div>
				<div class="delbtn">
				<a href="/del_gigs/{{_id}}"> <i  class = " fa fa-trash fa-lg"  aria-hidden= "true"></i> delete </a></div>
				</div>	


				</div>
				
				
				
			</div>
			{{/each}}
		</div>
		
		
	
